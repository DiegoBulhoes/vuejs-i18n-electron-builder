{
  "name": "vuejs-i18n-electron-builder",
  "version": "0.1.0",
  "private": true,
  "description": "Example of a Vue JS application with i18n and the electron-builder plugin",
  "author": "DiegoBulhoes",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DiegoBulhoes/vuejs-i18n-electron-builder.git"
  },
  "homepage": "https://github.com/DiegoBulhoes/vuejs-i18n-electron-builder/blob/master/README.md",
  "keywords": [
    "electron",
    "electron-builder",
    "vue",
    "vue-cli",
    "i18n",
    "jest",
    "vuetify"
  ],
  "scripts": {
    "vue:serve": "vue-cli-service serve",
    "vue:build": "vue-cli-service build",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix",
    "test": "vue-cli-service test:unit --testPathIgnorePatterns electron.spec.js",
    "test:all": "vue-cli-service test:unit",
    "test:watch": "vue-cli-service test:unit --watch",
    "test:watchAll": "vue-cli-service test:unit --watchAll",
    "test:cover": "vue-cli-service test:unit --coverage",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "vue": "^2.5.21",
    "vue-i18n": "^8.7.0",
    "vue-router": "^3.0.1",
    "vuetify": "^1.3.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-plugin-unit-jest": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "electron": "^3.0.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-electron-builder": "^1.0.0-rc.10",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.5.21",
    "vuetify-loader": "^1.0.5"
  },
  "main": "background.js"
}
